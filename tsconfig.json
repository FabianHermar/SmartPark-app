{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
			"@/*": ["./*", "./assets/*", "./components/*"],
			"@/constants/*": ["./constants/*"],
			"@/lib/*": ["./lib/*"]
		}
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
